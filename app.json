{
  "expo": {
    "scheme": "ginmeek-gamsavy",
    "name": "GamSavy",
    "slug": "gamsavy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/gamsavy_icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/gamsavy_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ccccff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/gamsavy_adaptive_icon.png",
        "backgroundColor": "#ccccff"
      },
      "package": "com.ginmeek.gamsavy"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"]
  }
}
